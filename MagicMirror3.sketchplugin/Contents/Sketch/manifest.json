                 "script" : "oncommand.js",
                 "handler" : "disable"
                 },
                 {
                 "script" : "onchange.js",
                 "handlers" : {
                     "actions" : {
                         "SelectionChanged.finish" : "onSelectionChanged",
                         "-ArtboardChanged.finish" : "onArtboardChanged",
                         "-LayersMoved.finish" : "onLayersMoved",
                         "-AddFill.finish": "onAddFill",
                         "-Edit.finish": "onEdit",
                         "-ToggleSelection.finish": "onToggleSelection",
                     }
                     },
                 },
    ],
	"menu": {
	    "isRoot" : false,
	    "shortcut" : "",
